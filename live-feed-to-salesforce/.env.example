# BuiltWith API Key - get yours at https://api.builtwith.com
BUILTWITH_API_KEY=your-api-key-here

# Comma-separated list of channels to subscribe to
# Options: technology names (Shopify, WordPress, Google-Analytics), or rule channels (new, new-historical, premium)
BUILTWITH_CHANNELS=new,Shopify

# Domain API enrichment options
DOMAIN_API_LIVEONLY=true
DOMAIN_API_NOPII=false
DOMAIN_API_NOMETA=false
DOMAIN_API_NOATTR=true

# Salesforce Connected App + user credentials
# Login URL: https://login.salesforce.com (production) or https://test.salesforce.com (sandbox)
SF_LOGIN_URL=https://login.salesforce.com
SF_API_VERSION=v61.0
SF_CLIENT_ID=your-connected-app-client-id
SF_CLIENT_SECRET=your-connected-app-client-secret
SF_USERNAME=your-salesforce-username
SF_PASSWORD=your-salesforce-password
SF_SECURITY_TOKEN=your-salesforce-security-token

# Lead behavior
SF_UPSERT_BY_WEBSITE=true
SF_LEAD_SOURCE=BuiltWith Live Feed
